@{
    ViewData["Title"] = "Home Page";
}
@model MapperProfileExample.Dto.UserDto;
    <div class="container">

        <h1> Selected Data</h1>

        <table class=" table table-bordered">
            <tr>
                <th>Name</th>
                <th>Surname</th>
                <th>Age</th>
            </tr>
            <tr>
                <td>@Model.Name</td>
                <td>@Model.SurName</td>
                <td>@Model.Age</td>
            <tr />



        </table>
        <hr />
        <h1> Information Datas</h1>

        <table class=" table table-bordered">

            <tr>
                <th>Name</th>
                <th>Surname</th>
                <th>City</th>
            </tr>

            @foreach (var item in Model.JsonString.data)
            {
                <tr>
                    <td>@item.Name</td>
                    <td>@item.Surname</td>
                    <td>@item.City</td>
                </tr>
            }



        </table>
    </div>
